    "workshopMaterialsDownloadUrl": "https://terrestris.github.io/docker-ws/materials.zip",
    "osGeoLiveVersion": "14.0",
    "authors": [{
      "name": "Jan Suleiman",
      "mail": "suleiman@terrestris.de"
    }, {
      "name": "Daniel Koch",
      "mail": "koch@terrestris.de"
    }]
